#%Module

module-whatis "Setup monthlyrpts paths and environemnt
variables for use."

proc ModulesHelp { } {
    puts stderr "Adds monthlyrpts to the search path."
    puts stderr ""
}

set  VERSION            2.0.0
set  MONTHLYRPTS_ROOT   /users/cardo/work/monthlyrpts/2.0.0
set  MONTHLYRPTS_RPTS   /users/cardo/work/monthlyrpts/2.0.0/reports
set  MONTHLYRPTS_PLOTS  /users/cardo/work/monthlyrpts/2.0.0/plots

append-path     PATH  /users/cardo/work/monthlyrpts/2.0.0/bin
append-path  MANPATH  /users/cardo/work/monthlyrpts/2.0.0/man

setenv  MONTHLYRPTS_ROOT   /users/cardo/work/monthlyrpts/2.0.0
setenv  MONTHLYRPTS_RPTS   /users/cardo/work/monthlyrpts/2.0.0/reports
setenv  MONTHLYRPTS_PLOTS  /users/cardo/work/monthlyrpts/2.0.0/plots

