	points: 7200(s) min(s) 7200(s) max(s)

#proc	line
	linedetails: width=1 color=coral
	points: 8640(s) min(s) 8640(s) max(s)

#proc	line
	linedetails: width=1 color=coral
	points: 10080(s) min(s) 10080(s) max(s)

#proc	line
	linedetails: width=1 color=coral
