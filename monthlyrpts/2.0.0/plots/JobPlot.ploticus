//
//  Read the data
//
#proc getdata
	file: @DATAFILE
	fieldnames: jobid submit eligible start end elapsed timelimit queuewait nodes nodetp partition

//
//  Define the plot area
//
#proc	areadef
	title:        @CUSTOMER Elapsed Time vs Node Count (@MONTH @YEAR)
	titledetails: size=14  align=L adjust=-0.5,0.25
	xrange:       0 1500
	yrange:       0 @MAXNODES
	frame:        no
 
//
//  Details for the X axis
//
#proc	xaxis
	location:     min
	stubs:        increment 60
	stubdetails:  adjust=0,+0.05 
	stubvert:     yes
	tics:         none
	axisline:     no
	label:        Elapsed Time in Minutes
	labeldistance: 0.5

//
//  Details for the Y axis
//
#proc	yaxis
	stubs:        increment @NODEINC
	gridskip:     min
	grid:         yes
	ticincrement: @NODEINC
	label:        Node Count
	labeldistance:  0.5

//
//  Plot the data
//
#proc	scatterplot
	xfield:	     elapsed
	yfield:      nodes
	symbol:     shape=pixdiamond color=blue radius=0.01

//
// CSCS Confidiential Notice
//
#proc annotate
	textdetails: size=8 style=I
	location:    min min-0.5          // Lower Left
	text:        CSCS Confidential

#proc annotate
	textdetails: size=8 style=I
	location:    max-0.4 max+0.2      // Upper Right
	text:        CSCS Confidential

#proc annotate
	textdetails: size=8 style=I
	location:    max-0.5 min-0.5      // Lower Right
	text:        (@CUSTOMER JobPlot @MAXNODES)

